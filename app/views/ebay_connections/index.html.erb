<p id="notice"><%= notice %></p>

<h1>Listing Ebay Connections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ebay_connections.each do |ebay_connection| %>
      <tr>
        <td><%= link_to 'Show', ebay_connection %></td>
        <td><%= link_to 'Edit', edit_ebay_connection_path(ebay_connection) %></td>
        <td><%= link_to 'Destroy', ebay_connection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ebay connection', new_ebay_connection_path %>
